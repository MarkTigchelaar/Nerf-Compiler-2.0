                "<main_y:8",
                ">main_assign_1:iPUSHc",
                "2",
                "iPUSHc",
                "30",
                "iSUB",
                "iPUSHc",
                "4",
                "iEXP",
                "iMOVE",
                "main_y",
                ">main_return_2:iPUSHv",
                "main_y",
                "JUMP",
                "return_main",
                ">return_main:HALT"
