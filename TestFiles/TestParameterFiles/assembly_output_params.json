                "<main_y:8",
                ">main_assign_1:iPUSHc",
                "2",
                "iPUSHc",
                "30",
                "iADD",
                "iPUSHc",
                "4",
                "iMULT",
                "iMOVE",
                "main_y",
                ">main_return_2:iPUSHv",
