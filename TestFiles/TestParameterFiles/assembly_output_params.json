    "tests": [
        {
            "test_file_name": "var_assign.nerf",
            "expected_output_per_line":
            [
                "<main_e:8",
                "<main_y:8",
                ">main_assign_1:iPUSHc",
                "0",
                "iMOVE",
                "main_e",
                ">main_assign_2:iPUSHc",
                "2",
                "iPUSHc",
                "3",
                "iPUSHv",
                "main_e",
                "iMULT",
                "iADD",
                "iMOVE",
                "main_y",
                ">main_return_3:iPUSHc",
                "0",
                "JUMP",
                "return_main",
                ">return_main:HALT"
            ]
        },
        {
            "test_file_name": "var_assign_mod.nerf",
            "expected_output_per_line":
            [
                ">main_return_1:iPUSHc",
                "11",
                "iPUSHc",
                "3",
                "iPUSHc",
                "3",
                "iMOD",
                "iADD",
                "JUMP",
                "return_main",
                ">return_main:HALT"
            ]
        },
        {
            "test_file_name": "var_assign_parens.nerf",
